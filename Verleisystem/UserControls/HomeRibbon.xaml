<UserControl x:Class="Verleihsystem.UserControls.HomeRibbon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Verleihsystem.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="800">
    <Grid>
            <StackPanel Orientation="Horizontal">
            <Button Width="100" Margin="10" Click="New_Click">Neu</Button>
            <Button Width="100" Margin="10" Click="Filter_Click">Filter</Button>
                <Button Width="100" Margin="10" Click="Refresh_Click">Refresh</Button>
            <Button Width="100" Margin="10" Click="Search_Click">Erweiterte Suche
            </Button>
        </StackPanel>
    </Grid>
</UserControl>
